            </column>
            <column name="issued_date" type="DATE">
                <constraints nullable="false"/>
            </column>
            <column name="expiration" type="DATE">
                <constraints nullable="false"/>
            </column>
            <column name="jshshir" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="address" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
